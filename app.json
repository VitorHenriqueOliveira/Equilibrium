      "package": "com.vitoncio.DigitalDetox"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f51b09ce-48b3-4579-8cb6-c9191233e0b0"
      }
